{
  "name": "Cubius",
  "version": "1.0.0",
  "description": "This is an open source project supported by the Vision Lab at Harvard University. Cubius is an experiment engine that runs psychology experiments online.",
  "author": "Na Li <linazhao128@gmail.com>",
  "main": "index.js",
  "scripts": {
    "compile": "better-npm-run compile",
    "start": "better-npm-run start",
    "deploy": "better-npm-run deploy",
    "deploy:dev": "better-npm-run deploy:dev",
    "deploy:prod": "better-npm-run deploy:prod"
  },
  "betterScripts": {
    "compile": {
      "command": "node bin/compile",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "start": {
      "command": "node bin/dev-server",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "deploy": {
      "command": "npm run compile",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "deploy:dev": {
      "command": "npm run deploy",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "app:*"
      }
    },
    "deploy:prod": {
      "command": "npm run deploy",
      "env": {
        "NODE_ENV": "production",
        "DEBUG": "app:*"
      }
    }
  },
  "license": "MIT",
  "devDependencies": {
    "babel-eslint": "^7.1.0",
    "browser-sync": "^2.18.6",
    "browser-sync-webpack-plugin": "^1.1.3",
    "eslint": "^3.13.1",
    "eslint-config-standard": "^6.2.1",
    "eslint-plugin-babel": "^4.0.0",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-standard": "^2.0.1",
    "expose-loader": "^0.7.1",
    "express": "^4.14.0",
    "nodemon": "^1.10.2",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.12.2"
  },
  "dependencies": {
    "@orange-games/phaser-input": "^2.0.2",
    "babel-core": "^6.21.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-polyfill": "^6.20.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-stage-0": "^6.3.13",
    "babel-runtime": "^6.11.6",
    "better-npm-run": "0.0.11",
    "compression": "^1.6.2",
    "debug": "^2.2.0",
    "file-loader": "^0.9.0",
    "fs-extra": "^0.30.0",
    "html-webpack-plugin": "^2.22.0",
    "imports-loader": "^0.6.5",
    "ip": "^1.1.2",
    "json-loader": "^0.5.4",
    "papaparse": "^4.3.2",
    "phaser-ce": "^2.7.3",
    "phaser-input": "^1.2.6",
    "url-loader": "^0.5.6",
    "webfontloader": "^1.6.27",
    "webpack": "^2.2.0",
    "yargs": "^6.3.0"
  }
}
